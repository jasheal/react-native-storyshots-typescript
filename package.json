{
  "name": "RNStoryShots",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "storybook": "storybook start --config-dir storybook"
  },
  "dependencies": {
    "jest-static-stubs": "^0.0.1",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "0.57.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addon-storyshots": "^4.0.4",
    "@storybook/addons": "^4.0.4",
    "@storybook/react-native": "^4.0.4",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.3",
    "@types/react-native": "^0.57.7",
    "@types/storybook__addon-storyshots": "^3.4.3",
    "@types/storybook__react": "^4.0.0",
    "@types/storybook__react-native": "^3.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "babel-runtime": "^6.26.0",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "0.49.0",
    "prop-types": "^15.6.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "16.6.0-alpha.8af6728",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "storybook-addon-jsx": "^5.4.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      ".+\\.(png)$": "jest-static-stubs/png"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  }
}
